<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.embed.swing.SwingNode?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="es.ciudadescolar.hotelreserva.ReportController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button fx:id="printButton" mnemonicParsing="false" onAction="#onPrintButtonClick" text="Print Button" />
            <Button fx:id="showButton" mnemonicParsing="false" onAction="#onShowButtonClick" text="Show Button" />
            <SwingNode fx:id="helpButtonNode" />
        </buttons>
    </ButtonBar>
    <StackPane fx:id="reportPanel" VBox.vgrow="ALWAYS" />

    <Label fx:id="info" />
</VBox>
